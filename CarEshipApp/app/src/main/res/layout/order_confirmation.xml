<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="27dp"
        android:layout_marginStart="9dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="48dp"
        android:fontFamily="@font/hammersmith_one"
        app:layout_constraintBottom_toTopOf="@+id/orderConfirmation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/orderConfirmation"
        app:srcCompat="@drawable/logo"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="367dp"
        android:layout_height="60dp"
        android:layout_marginTop="413dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/hammersmith_one"
        android:text="Your payment was successful"
        android:textColor="@color/careship_palette_raisin_black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="67dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="14dp"
        android:fontFamily="@font/hammersmith_one"
        android:textColor="@color/careship_palette_raisin_black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView19"
        app:layout_constraintEnd_toStartOf="@+id/dateText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/dateText"
        android:layout_width="0dp"
        android:layout_height="52dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="56dp"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />


    <TextView
        android:id="@+id/orderConfirmation"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="67dp"
        android:fontFamily="@font/hammersmith_one"
        android:text="Order Confirmation"
        android:textColor="@color/careship_palette_popstar"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonTrackOrder"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="180dp"
        android:layout_height="58dp"
        android:layout_marginTop="16dp"
        android:background="@color/careship_palette_beige"
        android:fontFamily="@font/hammersmith_one"
        android:text="Check Order"
        android:textAllCaps="false"
        android:textColor="@color/careship_palette_beige"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/dateText"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView19"
        app:strokeColor="#FFFFFF" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="0dp"
        android:layout_height="240dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="68dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orderConfirmation"
        app:srcCompat="@drawable/medicine_delivery" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="64dp"
        android:layout_height="0dp"
        android:layout_marginBottom="121dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/confirmation__1_" />

</androidx.constraintlayout.widget.ConstraintLayout>